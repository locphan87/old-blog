{
  "private": true,
  "name": "locphan-blog",
  "description": "Loc Phan's Blog",
  "version": "1.0.0",
  "author": "Loc Phan <phanhoangloc@gmail.com>",
  "license": "MIT",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier {src,content}/**/*.{js,jsx,ts,tsx,json} --write",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^2.7.3",
    "gatsby": "^2.25.3",
    "gatsby-plugin-google-analytics": "^2.4.1",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-netlify": "^2.4.0",
    "gatsby-plugin-offline": "^3.3.3",
    "gatsby-plugin-sitemap": "^2.5.1",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.13",
    "gatsby-remark-autolink-headers": "^2.7.0",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/ramda": "^0.27.32",
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ]
  }
}
